/*
#include <stdio.h>
 int main(void)
 {
	 int count=12,sum=0;

	 while (count<=421)
	 {
		 if(count%2==0)
			 sum=sum+count;
		 count++;
	 }

	 printf("Sum of the even numbers between 12 and 421 is %d\n",sum);
	 return 0;
 }
 */
  //QUESTION 5
  /*
  #include <stdio.h>
 int main(void)
 {
	 int n,person,age,countf30=0,countm50=0,totalage=0;
	 char gender;
	 printf("How many employees you would like to record? ");
	 scanf("%d",&n);

	 for(person=1;person<=n;person++)
	 {
		 printf("Enter the gender of employee (capital M or F) %d: ",person);
		 scanf(" %c",&gender);
		 printf("Enter the age of employee %d: ",person);
		 scanf("%d",&age);
		 totalage=totalage+age;
		 if ( gender == 'F' && age>30)
			 countf30++;
		 else if ( gender == 'M' && age<50)
			 countm50++;
		 
	 }
	 printf("Number of female employees older than 30 is %d\n",countf30);
	 printf("Number of male employees younger than 50 is %d\n",countm50);
	 printf("Average age of the employees %.2lf\n",(double)totalage/n);
	 return 0;
 }
 */
