/*
#include <stdio.h>
 int main(void)
 {
	 char letter;
	 int count=0,counta=0,counte=0,counti=0,counto=0,countu=0;

	 while (count<20)
	 {
		 printf("Enter a character: ");
		 scanf(" %c",&letter);

		 switch (letter)
		 {
		 case 'A':
		 case 'a':
			 counta++;
			 break;
		 case 'E':
	     case 'e':
			 counte++;
			 break;
		 case 'I':
		 case 'i':
			 counti++;
			 break;
	     case 'O':
		 case'o':
			 counto++;
			 break;
	     case 'U':
		 case'u':
			 counto++;
			 break;
		 }
		 count++;
	 }
	 printf("A repeated %d time(s)\n",counta);
	 printf("E repeated %d time(s)\n",counte);
	 printf("I repeated %d time(s)\n",counti);
	 printf("O repeated %d time(s)\n",counto);
	 printf("U repeated %d time(s)\n",countu);

	  return 0;
 }
 */
