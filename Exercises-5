 /*
 #include <stdio.h>
 int main(void)
 {
	 int count,number,maximum;
	 
	 printf("Enter an integer number: ");
	 scanf("%d",&number);
	 maximum=number;
	 for(count=1;count<10;count++)
	 {
		 printf("Enter an integer number: ");
	     scanf("%d",&number);
		 if (number>maximum)
			 maximum=number;
	 }

	 printf("Maximum of the 10 integers is %d\n",maximum);

	  return 0;
 }
 */
  //QUESTION 7
  /*
  #include <stdio.h>

   int main(void)
   {
	 int number,original,last,sum=0,digitcount=0;
	 printf("Enter the number: ");
	 scanf("%d",&number);
	 original=number;
	 
	 while(number != 0)
	 {
		 last=number%10;
		 number=number/10;
		 sum=last+sum;
		 digitcount++;
	 }

	 printf("Entered number has %d digits\n",digitcount);
	 printf("Sum of its digits %d\n",sum);
	  return 0;
    }
 */



